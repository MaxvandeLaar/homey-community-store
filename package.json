{
  "name": "homey-community-store",
  "productName": "Homey Community Store",
  "version": "0.2.0",
  "description": "The Homey Community Store provides an alternative app store next to the Offical Athom Store",
  "main": ".webpack/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaxvandeLaar/homey-community-store.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "make": "electron-forge make --targets=darwin,linux,win32",
    "make:update:darwin": "cd out/Homey\\ Community\\ Store-darwin-x64/Homey\\ Community\\ Store.app/Contents/Resources/app && zip -r update-source.zip ./",
    "make:update:linux": "cd out/Homey\\ Community\\ Store-linux-x64/resources/app && zip -r update-source.zip ./",
    "make:update:win": "cd out/Homey\\ Community\\ Store-win32-x64/resources/app && zip -r update-source.zip ./",
    "make:darwin": "electron-forge make --platform=darwin --arch=x64",
    "pack:win": "electron-forge package --platform=win32 --arch=x64",
    "make:win": "electron-forge make --platform=win32 --arch=x64",
    "make:win:skip": "electron-forge make --platform=win32 --arch=x64 --skip-package",
    "make:win:mac": "npm run pack:win && mkdir -p out/make/squirrel.windows/x64",
    "postmake:win:mac": "cd out && zip -r Homey\\ Community\\ Store-win32-x64.zip ./Homey\\ Community\\ Store-win32-x64 && mv ./Homey\\ Community\\ Store-win32-x64.zip ./make/squirrel.windows/x64/" ,
    "make:linux": "electron-forge make --platform=linux --arch=x64"
  },
